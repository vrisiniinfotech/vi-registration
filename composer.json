{
	"name":"vrisini/vi-registration",
	"description":"Erweitern Sie die Contao-Registrierung um erweiterte Funktionen wie die codebasierte E-Mail-Validierung, etc.",
	"keywords":["contao","registrierung","E-Mail-Validierung","form","codebasierte","vrisini"],
	"type":"contao-bundle",
	"homepage":"https://www.vrisini.de",
	"license":"MIT",
	"authors":[
		{
			"name": "Anupam Chatterjee",
			"email": "anupam@vrisini.de",
			"homepage": "https://www.vrisini.de",
			"role": "Developer"
		}
	],
	"support": {
        "issues": "https://github.com/vrisiniinfotech/vi-registration/issues",
        "source": "https://github.com/vrisiniinfotech/vi-registration",
        "forum": "https://community.contao.org"
    },
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "^4.5",
        "doctrine/dbal": "^2.7",
        "psr/log": "^1.0",
        "symfony/config": "^3.3 || ^4.0",
        "symfony/dependency-injection": "^3.3 || ^4.0",
        "symfony/event-dispatcher": "^3.3 || ^4.0",
        "symfony/http-foundation": "^3.3 || ^4.0",
        "symfony/http-kernel": "^3.3 || ^4.0",
        "symfony/security-core": "^3.3 || ^4.0",
        "symfony/security-http": "^3.3 || ^4.0",
        "composer/package-versions-deprecated": ">=1.0"
    },
    "require-dev": {
        "contao/manager-plugin": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Vrisini\\ViRegistrationBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Vrisini\\ViRegistrationBundle\\ContaoManager\\Plugin"
    }
}
